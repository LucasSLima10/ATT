var animais = [
  ["Leão", "Felino", 8],
  ["Elefante", "Mamífero", 15],
  ["Girafa", "Mamífero", 10],
  ["Tigre", "Felino", 6],
  ["Urso", "Mamífero", 12]
];


console.log("Nome\t\tEspécie\t\tIdade");
for (var i = 0; i < animais.length; i++) {
  console.log(animais[i][0] + "\t\t" + animais[i][1] + "\t\t" + animais[i][2]);
}
