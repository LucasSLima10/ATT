var nomes = ["Ana", "Carlos", "Elena", "Fernando", "Gabriela", "Hugo", "Isabel", "João", "Karen", "Lucas"];
var idades = [25, 32, 28, 40, 22, 29, 37, 31, 26, 34];
var coresFavoritas = ["azul", "verde", "rosa", "amarelo", "vermelho", "laranja", "roxo", "preto", "branco", "marrom"];


